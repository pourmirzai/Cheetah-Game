{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/((?!assets|favicon.ico|robots.txt|manifest.json|sitemap.xml).*)",
      "destination": "/api/index"
    }
  ]
}
